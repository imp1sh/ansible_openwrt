# Distributed by Ansible
# Wifi devices
{% for key, value in openwrt_wifi_devices.items() %}
config wifi-device "{{ key }}"
{% if value.type is defined %}
	option type "{{ value.type }}"
{% endif %}
{% if value.phy is defined %}
	option phy "{{ value.phy }}"
{% endif %}
{% if value.macaddr is defined %}
	option macaddr "{{ value.macaddr }}"
{% endif %}
{% if value.disabled is defined %}
	option disabled "{{ value.disabled }}"
{% endif %}
{% if value.channel is defined %}
	option channel "{{ value.channel }}"
{% endif %}
{% if value.channels is defined %}
	option channels "{{ value.channels }}"
{% endif %}
{% if value.hwmode is defined %}
	option hwmode "{{ value.hwmode }}"
{% endif %}
{% if value.htmode is defined %}
	option htmode "{{ value.htmode }}"
{% endif %}
{% if value.chanbw is defined %}
	option chanbw "{{ value.chanbw }}"
{% endif %}
{% if value.ht_capab is defined %}
	option ht_capab "{{ value.ht_capab }}"
{% endif %}
{% if value.txpower is defined %}
	option txpower "{{ value.txpower }}"
{% endif %}
{% if value.diversity is defined %}
	option diversity "{{ value.diversity }}"
{% endif %}
{% if value.rxantenna is defined %}
	option rxantenna "{{ value.rxantenna }}"
{% endif %}
{% if value.txantenna is defined %}
	option txantenna "{{ value.txantenna }}"
{% endif %}
{% if value.country is defined %}
