{% endif %}
{% if value.beacon_int is defined %}
	option beacon_int "{{ value.beacon_int }}"
{% endif %}
{% if value.legacy_rates is defined %}
	option legacy_rates "{{ value.legacy_rates }}"
{% endif %}
{% if value.require_mode is defined %}
