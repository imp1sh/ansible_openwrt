{% endif %}
{% if value.chanbw is defined %}
	option chanbw "{{ value.chanbw }}"
{% endif %}
{% if value.noscan is defined %}
	option noscan "{{ value.noscan }}"
{% endif %}
{% if value.ht_coex is defined %}
	option ht_coex "{{ value.ht_coex }}"
{% endif %}
{% if value.frag is defined %}
	option frag "{{ value.frag }}"
{% endif %}
{% if value.rts is defined %}
	option rts "{{ value.rts }}"
{% endif %}
{% if value.antenna_gain is defined %}
	option antenna_gain "{{ value.antenna_gain }}"
{% endif %}
{% if value.frameburst is defined %}
	option frameburst "{{ value.frameburst }}"
{% endif %}
{% if value.maxassoc is defined %}
